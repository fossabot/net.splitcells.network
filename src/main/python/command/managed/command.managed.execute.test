if [[ "$(command.managed.execute disjunction unknown-command)" != *'Could not execute "unknown-command".'* ]]; then
	echo.error '"command.managed.execute" executed not present command in disjunction.'
	exit 1
fi
if [[ "$(command.managed.execute conjunction unknown-command)" != *'Could not execute "unknown-command".'* ]]; then
	echo.error '"command.managed.execute" executed not present command in conjunction.'
	exit 1
fi
